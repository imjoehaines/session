{
    "name": "imjoehaines/session",
    "description": "A simple PHP session abstraction library",
    "keywords": ["session"],
    "homepage": "https://github.com/imjoehaines/session",
    "license": "Unlicense",
    "authors": [
        {
            "name": "Joe Haines",
            "email": "hello@joehaines.co.uk",
            "homepage": "https://www.joehaines.co.uk",
            "role": "Developer"
        }
    ],
    "scripts": {
        "test": "composer test:unit && composer test:phpstan",
        "test:unit": "phpunit tests",
        "test:phpstan": "phpstan analyse src tests"
    },
    "autoload": {
        "psr-4": {
            "Imjoehaines\\Session\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imjoehaines\\Session\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "7.4.4",
        "phpstan/phpstan": "0.11.15",
        "phpstan/phpstan-doctrine": "0.10.0",
        "phpstan/phpstan-deprecation-rules": "0.10.2",
        "phpstan/phpstan-strict-rules": "0.10.1"
    }
}
